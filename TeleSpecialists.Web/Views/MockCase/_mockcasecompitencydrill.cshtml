            @Html.Label("mcas_nurse_identify_members_in_room_true", "Yes")
            @Html.RadioButtonFor(model => model.mcas_nurse_identify_members_in_room, false, new { id = "mcas_nurse_identify_members_in_room_false", @class = "disabled ml-3" })
            @Html.Label("mcas_nurse_identify_members_in_room_false", "No")
        </div>
    </div>
    <div class="col-12 row mb-1 ml-1">
