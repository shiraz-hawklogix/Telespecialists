                </div>

                <div class="form-group row">
                    @if (Model.cas_premorbid_symptoms == 6)
                    {
                        <input type="radio" name="cas_premorbid_symptoms" id="cas_metric_has_morbid_symptoms_6_UP" value="6" style="margin-left:10px; margin-top:05px" checked />
                    }
