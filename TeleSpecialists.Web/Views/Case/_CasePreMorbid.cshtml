                        @if (Model.cas_premorbid_symptoms == 7)
                        {
                            <input type="radio" name="cas_premorbid_symptoms" id="cas_metric_has_morbid_symptoms_7_UP" value="7" style="margin-left:10px; margin-top:05px" checked />
                        }
