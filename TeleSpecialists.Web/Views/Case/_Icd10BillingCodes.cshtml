<div class="form-group">
    <div class="row">
        <label class="control-label col-12">Search Diagnosis Codes: </label>

        <div class="col-12">
            <div id="icd10SearchKeys" style="display:none;">
            </div>
        </div>

        <div class="col-12">
            <input type="text" id="icd10diagnosis" placeholder="Code or name" class="search_field" autocomplete="off">
            <div id="IcdDiagnoseCodesId" style="display:none"></div>
        </div>

        <div id="icdCal10Div" class="col-12" style="display:none;">
            <label class="col-12" style="font-size:12px;margin-bottom:0px;padding-left:0px;">Select a specific daignosis: </label>
            <div class="row" id="icd10Cal">
                <div class="col-3 pl-1 pr-0" id="parent_1" style="display:none;">
                </div>
                <div class="col-9 pl-0 pr-0" id="parent_child_1" style="display:none;">
                </div>
                <hr />
                <div class="col-3 pl-1 pr-0" id="parent_2" style="display:none;">
                </div>
                <div class="col-9 pl-0 pr-0" id="parent_child_2" style="display:none;">
                </div>
                <hr />
                <div class="col-3 pl-1 pr-0" id="parent_3" style="display:none;">
                </div>
                <div class="col-9 pl-0 pr-0" id="parent_child_3" style="display:none;">
                </div>
                <div class="col-3 pl-1 pr-0" id="parent_4" style="display:none;">
                </div>
                <div class="col-9 pl-0 pr-0" id="parent_child_4" style="display:none;">
                </div>
            </div>
        </div>

        <div class="row col-12 mt-2 pr-0">
            <div class="col-3">
                <label class="" style="font-size:12px;">Visit Daignosis:</label>
            </div>
            <div class="col-9 pl-0 pr-0">
                <input type="text" readonly title="" id="VisitDiagnosis" class="form-control" autocomplete="off" style="font-size:11px;">
            </div>
        </div>

        <div class="col-12 mt-2 pl-2">
            <a id="AcceptDiagnosisCode" style="cursor:pointer;color:white;" disabled class="btn btn-dark ml-2">Accept</a>
            <a id="ClearDiagnosisCode" style="cursor:pointer;color:white;" class="btn btn-dark ml-2">Clear</a>
        </div>

        <div class="col-12 mt-2">
            <div id="cas_billing_codes_temp"></div>
            @*<label style="font-size:13px;">Select a specific diagnosis:</label>
                <div class="specific-diagnosis-code"></div>*@
        </div>
    </div>
</div>
