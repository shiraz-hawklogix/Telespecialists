@using TeleSpecialists.BLL.Helpers;
@model List<TeleSpecialists.BLL.Model.entity_note>

<div class="modal-dialog modal-lg" id="">
    <div class="modal-content">
        <div>
            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Notes</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <div class="modal-body">
                @if (Model.Count() > 0)
                {
                    <div id="">
                        <table id="" class="table table-striped  table-sm" cellspacing="0" width="100%">
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@Html.Raw(item.etn_notes.Replace("\r\n","<br/>"))</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>

                }
            </div>
            <div class="modal-footer">
                <input type="button" value="Close" class="btn btn-dark" data-dismiss="modal"  />             
            </div>
        </div>
    </div>
</div>